    <key name="sync-provider" type="i">
      <default>0</default>
    </key>
    <key name="sync-url" type="s">
      <default>""</default>
    </key>
    <key name="sync-username" type="s">
      <default>""</default>
    </key>
    <key name="task-show-progressbar" type="b">
